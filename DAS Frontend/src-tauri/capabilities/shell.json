{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "shell",
  "description": "Shell capabilities for running sidecar binaries",
  "windows": ["main"],
  "permissions": [
    "shell:allow-open",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "das-backend",
          "sidecar": true
        }
      ]
    }
  ]
}
